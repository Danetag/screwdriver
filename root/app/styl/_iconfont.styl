@font-face {
	font-family: "iconfont";
	src: url('../static/fonts/icons/iconfont.eot');
	src: url('../static/fonts/icons/iconfont.eot?#iefix') format('eot'),
		url('../static/fonts/icons/iconfont.woff') format('woff'),
		url('../static/fonts/icons/iconfont.ttf') format('truetype'),
		url('../static/fonts/icons/iconfont.svg#iconfont') format('svg');
}

.icon:before {
	font-family: "iconfont";
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
	font-style: normal;
	font-variant: normal;
	font-weight: normal;
	/* speak: none; only necessary if not using the private unicode range (firstGlyph option) */
	text-decoration: none;
	text-transform: none;
}


.icon-back-arrow:before {
	content: "\E001";
}
